# AMBIGUOUS REDIRECT #######################################################

bash
bszilas@c3r10p6:~/m3/gh_msh/Beni$ echo hi > "a b c"
bszilas@c3r10p6:~/m3/gh_msh/Beni$ cat "a b c"
hi
bszilas@c3r10p6:~/m3/gh_msh/Beni$ export VAR="x y z"
bszilas@c3r10p6:~/m3/gh_msh/Beni$ echo hi > $VAR
bash: $VAR: ambiguous redirect
bszilas@c3r10p6:~/m3/gh_msh/Beni$ echo $?
1
bszilas@c3r10p6:~/m3/gh_msh/Beni$ export VAR="abc"
bszilas@c3r10p6:~/m3/gh_msh/Beni$ echo hi > $VAR
bszilas@c3r10p6:~/m3/gh_msh/Beni$ cat abc
hi
